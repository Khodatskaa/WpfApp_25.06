<Window x:Class="WpfApp_25._06.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp_25._06"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Name="StartServerButton" Content="Start Server" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Margin="10" Click="StartServerButton_Click"/>
        <Button Name="StopServerButton" Content="Stop Server" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Margin="120,10,0,0" Click="StopServerButton_Click" IsEnabled="False"/>
        <ListBox Name="LogListBox" HorizontalAlignment="Left" Height="200" VerticalAlignment="Top" Width="480" Margin="10,50,0,0"/>
    </Grid>
</Window>
